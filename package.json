{
	"name": "evening",
	"module": "src/index.ts",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "bun run src/reddit/index.ts",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"db:init": "bun run initialize.ts",
		"db:inspect": "bun run src/core/cache/inspect.ts",
		"db:generate": "drizzle-kit generate --dialect sqlite --schema ./src/core/cache/db.ts",
		"db:migrate": "bun run migrate.ts",
		"mastra:dev": "mastra dev",
		"mastra:build": "mastra build"
	},
	"devDependencies": {
		"@types/bun": "^1.3.2",
		"@types/node": "^24.10.1"
	},
	"peerDependencies": {
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"@biomejs/biome": "^2.3.5",
		"@mastra/core": "^0.24.1",
		"@total-typescript/tsconfig": "^1.0.4",
		"drizzle-kit": "^0.31.7",
		"drizzle-orm": "^0.44.7",
		"pino": "^10.1.0",
		"quicktype-core": "^23.2.6",
		"zod": "^4.1.12"
	}
}
